<Window x:Class="Стройматериалы.View.Info"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Стройматериалы.View"
        mc:Ignorable="d"
        Title="Info" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Данные будут загружены позже для просмотра!" FontSize="30"></TextBlock>
            <Button Height="40" Margin="10" Content="Вернуться назад" FontSize="20" Click="Button_Click" ></Button>
        </StackPanel>
    </Grid>
</Window>
